{
  "expo": {
    "jsEngine": "hermes",
    "name": "カチッ",
    "slug": "kachi-xtu",
    "version": "1.0.12",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "ITSAppUsesEncryption": true,
        "ITSAppUsesNonExemptEncryption": false,
        "UIDeviceFamily": [
          1
        ],
        "aps-environment": "production",
        "NSPhotoLibraryUsageDescription":"このアプリは次の目的で写真ライブラリにアクセスします:\n -アイコンの変更の際の新しいアイコンの取得のため\n -下書きに画像を追加するため"
      },
      "bundleIdentifier": "com.oosakanoakuma.kachi-xtu",
      "buildNumber": "112",
      "icon": "./assets/icon.png",
      "supportsTablet": false,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oosakanoakuma.kachi_xtu",
      "icon": "./assets/icon.png"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ],
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "a8ee13c2-aff0-49c2-b318-9a7c514c46ab"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/a8ee13c2-aff0-49c2-b318-9a7c514c46ab"
    }
  }
}
